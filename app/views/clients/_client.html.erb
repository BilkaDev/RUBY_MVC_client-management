<div id="<%= dom_id client %>">
  <p>
    <strong>Fullname:</strong>
    <%= client.fullname %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= client.email %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= client.notes %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= client.phone_number %>
  </p>

  <p>
    <strong>String:</strong>
    <%= client.string %>
  </p>

</div>
